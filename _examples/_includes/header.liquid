<!DOCTYPE html>
<html lang="en-us">
  <head>
    <title>
      {% if title %}
        {{ title }} &middot; {{ site.title }}
      {% else %}
        {{ site.title }}
      {% endif %}
    </title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    {% if config.cloud_typography_key and config.cloud_typography_key.size > 1 %}
    <link rel="stylesheet" type="text/css" id="js-webfont-link" href="{{ config.cloud_typography_key }}" data-href-premium="{{ config.cloud_typography_key }}" data-href-fallback="">
    {% endif %}
    <link rel="stylesheet" href="{% if page.url != '/' %}../{% endif %}../dist/css/framework.min.css">
  </head>
  <body class="pb-5">
